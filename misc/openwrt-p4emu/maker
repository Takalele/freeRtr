all:
	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) -Wall -o p4emu.bin $^ $(LDLIBS) -DHAVE_NOCRYPTO p4emu_pcap.c p4emu_full.c -lpthread -lpcap
	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) -Wall -o p4pkt.bin $^ $(LDLIBS) -DHAVE_NOCRYPTO p4emu_pcap.c p4emu_none.c -lpthread -lpcap
	$(CC) $(CPPFLAGS) $(CFLAGS) $(LDFLAGS) -Wall -o p4dbg.bin $^ $(LDLIBS) -DHAVE_NOCRYPTO p4emu_pcap.c p4emu_dbg.c -lpthread -lpcap
