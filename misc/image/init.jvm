#!/bin/sh
sh /init.sys
sh /init.dev
###rootfs###
cd /rtr
cp /proc/net/dev hwdet.eth
grep "rx:" /proc/tty/driver/serial | sed "s/^/\/dev\/ttyS/" > hwdet.ser
ip link show > hwdet.mac
if [ ! -f /rtr/hwdet-all.sh ] ; then
  java -jar /rtr/rtr.jar test hwdet tuntap 10.255.255.1/24 10.255.255.254 tcpvrf 23 host 23 path /rtr/ iface map line raw inline
  fi
chmod 755 *.sh
/rtr/hwdet-all.sh
while (true); do
  sleep 1
  done
exit 0
