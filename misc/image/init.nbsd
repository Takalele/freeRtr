#!/bin/sh

export PATH=/sbin:/bin:/usr/sbin:/usr/bin

mount -t tmpfs tmpfs /tmp
mount -t procfs procfs /proc
mount -t kernfs kernfs /sys
mount -t tmpfs -o union tmpfs /rtr
modload compat_linux

cd /rtr/
ifconfig -a > /rtr/hwdet.eth
echo "/dev/tty00: " > /rtr/hwdet.ser

/java/bin/java -jar /rtr/rtr.jar test hwdet path /rtr/ iface cmp line raw busywait java /java/bin/
chmod 755 /rtr/*.sh
/rtr/hwdet-all.sh
