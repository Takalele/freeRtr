Function rotateLeft(d,s,b:LongInt):LongInt;
Var m:LongInt;
Begin;
m:=(1 shl s)-1;
if (s>=32) then begin; s:=32;m:=$ffffffff; end;
b:=b mod s;
d:=d and m;
d:=(d shl b) or (d shr (s-b));
d:=d and m;
rotateLeft:=d;
End;

Function rotateRight(d,s,b:LongInt):LongInt;
Var m:LongInt;
Begin;
m:=(1 shl s)-1;
if (s>=32) then begin; s:=32;m:=$ffffffff; end;
b:=b mod s;
d:=d and m;
d:=(d shr b) or (d shl (s-b));
d:=d and m;
rotateRight:=d;
End;
